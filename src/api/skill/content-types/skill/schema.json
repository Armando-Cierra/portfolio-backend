{
  "kind": "collectionType",
  "collectionName": "skills",
  "info": {
    "singularName": "skill",
    "pluralName": "skills",
    "displayName": "Skills",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "pluginOptions": {},
      "type": "string",
      "required": true
    },
    "percentage": {
      "pluginOptions": {},
      "type": "integer",
      "required": true
    },
    "icon": {
      "pluginOptions": {},
      "type": "string",
      "required": true
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "web",
        "design"
      ]
    },
    "color": {
      "type": "customField",
      "regex": "^#([A-Fa-f0-9]{6}|[A-Fa-f0-9]{3})$",
      "customField": "plugin::color-picker.color"
    },
    "position": {
      "type": "decimal"
    }
  }
}
